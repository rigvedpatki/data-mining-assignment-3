{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": ["dom", "es2017", "esnext.asynciterable"],
    "outDir": "build",
    "rootDir": "src",
    "plugins": [
      {
        "name": "tslint-language-service"
      }
    ],
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "noUnusedLocals": false
  },
  "include": ["src"]
}
